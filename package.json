{
  "name": "awyes",
  "version": "1.0.0",
  "description": "gRPC service definition and utilities for AWS infrastructure automation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:proto": "protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=./src/generated --ts_proto_opt=esModuleInterop=true ./proto/awyes.proto",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build:proto && npm run build",
    "test": "jest"
  },
  "keywords": [
    "aws",
    "infrastructure",
    "automation",
    "grpc",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.9.13",
    "long": "^5.2.3",
    "protobufjs": "^7.2.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-proto": "^1.165.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "proto",
    "README.md"
  ]
}
